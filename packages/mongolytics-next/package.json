{
  "name": "@clipper-dev/mongolytics-next",
  "version": "1.0.7",
  "description": "Self-hosted, MongoDB-powered analytics for React/Next.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "exports": {
    "./client": {
      "import": "./dist/client/index.js",
      "require": "./dist/client/index.js", 
      "types": "./dist/client/index.d.ts"
    },
    "./server": {
      "import": "./dist/server/index.js",
      "require": "./dist/server/index.js",
      "types": "./dist/server/index.d.ts"
    },
    "./dashboards": {
      "import": "./dist/dashboards/index.js",
      "require": "./dist/dashboards/index.js",
      "types": "./dist/dashboards/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "client": ["./dist/client/index.d.ts"],
      "server": ["./dist/server/index.d.ts"],
      "dashboards": ["./dist/dashboards/index.d.ts"],
      "types": ["./dist/types.d.ts"]
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "mongolytics",
    "analytics",
    "mongodb",
    "nextjs",
    "react",
    "self-hosted",
    "next"
  ],
  "author": "Eric Kulbiej",
  "license": "MIT",
  "peerDependencies": {
    "next": "^12.0.0 || ^13.0.0 || ^14.0.0 || ^15.0.0",
    "react": "^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "mongodb": "^6.19.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "typescript": "^5.0.0"
  },
  "homepage": "https://www.mongolytics.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/clipper-dev/mongolytics.git"
  },
  "bugs": {
    "url": "https://github.com/clipper-dev/mongolytics/issues"
  }
}